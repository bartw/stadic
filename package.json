{
  "name": "stadic",
  "version": "0.0.3",
  "description": "A very basic React static site generator",
  "main": "lib/index.js",
  "scripts": {
    "start": "babel-node src/index.js --html ./example/index.html --ouput ./public --pages ./example/pages",
    "build": "babel src --out-dir lib"
  },
  "keywords": [
    "stadic",
    "react",
    "static",
    "generator",
    "website"
  ],
  "author": "Bart Wijnants",
  "license": "MIT",
  "dependencies": {
    "jsdom": "^16.2.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1"
  }
}
